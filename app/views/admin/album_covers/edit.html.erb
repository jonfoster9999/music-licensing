<%= image_tag @cover.file_name, :height => "200px" %>
<%= form_for @cover, :url => "/admin/album_covers/#{@cover.id}", :method => :patch do |f| %>

<%= f.collection_check_boxes(:song_ids, Song.all, :id, :title) do |b| %>
        <%= b.label class:"label-checkbox" do%>
         <%=b.check_box + b.text%>
    <% end %>
    <% end %>
<%= f.submit %>

<% end %>