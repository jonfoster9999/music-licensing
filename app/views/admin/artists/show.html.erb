<%= link_to "Back", admin_artists_path %>
<h2><%= @artist.name %></h2>
<h3>Description:</h3>
	<p><%= @artist.description %></p>
	<%= link_to "Edit artist description", edit_admin_artist_path(@artist) %>
<h3>Songs:</h3>
<ul>
	<% @artist.songs.each do |song| %>
		<li><%= song.title %></li>
	<% end %>
</ul>
