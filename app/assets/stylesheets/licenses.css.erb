.paper {
  background-size: 600px;
  width: 400px;
  background-image: url(https://image.freepik.com/free-vector/grey-linen-texture-background_1053-253.jpg);
  background-position: -45px;
  background-repeat: no-repeat;
  border: 1px solid black;
  margin-left: auto;
  margin-right: auto;
  margin-top: -20px;
  text-align: center;
  margin-bottom: 8px;
}

.under__pic {
	text-align: center;
	margin-bottom: 0px;
	padding-left: 4px;
	padding-right: 4px;
}


.paper ul {
	list-style: none;
}

.paper h1 {
	margin-top: 30px;
}

html {
	height: 100%;
}

.official {
	color: black;
	background-image: url(https://image.freepik.com/free-vector/grey-linen-texture-background_1053-253.jpg);
	border: 1px solid black;
}

body {
	height: 100%;
	background-image: url("<%= asset_path 'licensing.jpg' %>");
	background-size: cover;
	text-align: center;
}

